{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "npx concurrently --kill-others \"npm run dev:only\" \"npm run serve:dummy\"",
    "dev:only": "nuxt dev --host",
    "serve:dummy": "npx json-server --watch .dummy/db.json --port 3113 --host 0.0.0.0",

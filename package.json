    "@nuxt/icon": "^1.11.0",
    "@nuxt/ui": "^3.0.0",
    "@vueuse/core": "^13.0.0",
    "eslint": "^9.22.0",
    "firebase": "^11.4.0",
    "json-server": "^0.17.4",
    "lodash": "^4.17.21",
    "nuxt": "^3.16.0",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "concurrently": "^9.1.2"
  }
}
